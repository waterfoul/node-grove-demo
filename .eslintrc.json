{
  "parser": "babel-eslint",
  "plugins": [
    "flowtype",
    "react"
  ],
  "env": {
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:flowtype/recommended",
    "plugin:react/recommended"
  ],
  "rules": {
    "flowtype/require-valid-file-annotation": [
      2,
      "always"
    ],
    "react/prop-types": 0
  }
}